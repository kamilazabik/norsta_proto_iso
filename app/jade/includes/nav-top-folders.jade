// Brand and toggle get grouped for better mobile display
.navbar-header.fixed-brand.navbar-folders
  button#menu-toggle.navbar-toggle.collapsed(type='button', data-toggle='collapse'  )
    span.glyphicon.glyphicon-forward(aria-hidden='true')
  a#logo.navbar-brand(href='#')
    img(alt='Brand', src='images/argevide_logo.png')
  button#main-menu.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-navbar-collapse-2', aria-expanded='false')
    span.sr-only Toggle navigation
    span.icon-bar
    span.icon-bar
    span.icon-bar
// navbar-header
#bs-example-navbar-collapse-1.collapse.navbar-collapse
  ul.nav.navbar-nav
    li.active
      button#menu-toggle-2.navbar-toggle.collapse.in(data-toggle='collapse')
        span.glyphicon.glyphicon-forward(aria-hidden='true')
  // navbar-header
#bs-navbar-collapse-2.collapse.navbar-collapse
  ul.nav.navbar-nav
    li.dropdown
