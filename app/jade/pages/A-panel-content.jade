.col-md-9.col-sm-12.col-xs-12.pull-left.padding-content
  //button.btn.btn-primary.btn-lg(type='button', data-toggle='modal', data-target='#myModal')
  //  | Launch demo modal
  //// Modal
  #myModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='myModalLabel')
    .modal-dialog(role='document')
      .modal-content
        .modal-header
          button.close(type='button', data-dismiss='modal', aria-label='Close')
            span(aria-hidden='true') ×
          h4#myModalLabel.modal-title Modal title
        .modal-body Czy chcesz zapisać zmiany?
        .modal-footer
          //button.btn.btn-default(type='button', data-dismiss='modal') Close
          //button.btn.btn-primary(type='button') Save changes
          button(type="button" id="removeAssess" class="btn btn-primary btn-sm pull-right") Anuluj ocenę
          button(type="button" id="saveAssess" class="btn btn-primary btn-sm pull-right") Zapisz ocenę


include ../right-panels



