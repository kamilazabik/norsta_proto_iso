.col-md-9.col-sm-12.col-xs-12.pull-left.padding-content
      //button.btn.btn-primary.btn-lg(type='button', data-toggle='modal', data-target='#myModal')
      //  | Launch demo modal
      //// Modal
    #myModal.modal.fade.bs-example-modal-sm(tabindex='-1', role='dialog', aria-labelledby='myModalLabel')
      .modal-dialog.modal-sm(role='document')
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') ×
            h4#myModalLabel.modal-title Potwierdzenie
          .modal-body Czy chcesz zapisać zmiany?
          .modal-footer
            //button.btn.btn-default(type='button', data-dismiss='modal') Close
            //button.btn.btn-primary(type='button') Save changes
            button(type="button" id="removeAssessAlert" class="btn btn-primary btn-sm pull-right") Anuluj ocenę
            button(type="button" id="saveAssessAlert" class="btn btn-primary btn-sm pull-right") Zapisz ocenę

    .btn-group(data-toggle='buttons')
      label.btn.btn-primary.active
        input#option1(type='radio', name='options', autocomplete='off', checked='')
        | Radio 1
      label.btn.btn-primary
        input#option2(type='radio', name='options', autocomplete='off')
        |  Radio 2



include ../right-panels



