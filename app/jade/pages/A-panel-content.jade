          label(for='textareaNameEvid') Nazwa
          textarea#textareaNameEvid.form-control(rows='1' placeholder='Wpisz nazwę')
        .form-group
          input.file(type='file', name='')
          .input-group.col-xs-12
            input.form-control.name-evid(type='text', disabled='', placeholder='Dodaj dokument')
